{
  "name": "linkedin-backend",
  "version": "1.0.0",
  "description": "Node.js backend for storing user details and LinkedIn tokens",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "worker": "node startWorker.js",
    "worker:dev": "nodemon startWorker.js",
    "worker:linkedpost": "node startLinkedPostRefinementWorker.js",
    "worker:linkedpost:dev": "nodemon startLinkedPostRefinementWorker.js",
    "migrate:indexes": "node migrations/addIndexes.js",
    "migrate:userid": "node migrations/add-userId-to-posts.js",
    "migrate:all": "npm run migrate:userid && npm run migrate:indexes",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "nodejs",
    "express",
    "mongodb",
    "linkedin",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@bull-board/api": "^6.12.1",
    "@bull-board/express": "^6.12.1",
    "bcryptjs": "^2.4.3",
    "bull-board": "^1.7.2",
    "bullmq": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.0.1",
    "generic-pool": "^3.9.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.3.2",
    "ioredis-mock": "^8.9.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.5",
    "node-fetch": "^3.3.2",
    "openai": "^4.28.0",
    "prom-client": "^15.1.3",
    "puppeteer": "^22.15.0",
    "rate-limiter-flexible": "^7.2.0",
    "uuid": "^9.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "autocannon": "^8.0.0",
    "nodemon": "^3.1.10"
  }
}
